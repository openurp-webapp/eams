<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.eams.grade.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
  
  
    <class name="CourseGradeStateBean" entity-name="org.openurp.eams.grade.CourseGradeState" >
        <cache usage="read-write" region="org.openurp.eams.grade.model"/>
        <id name="id" unsaved-value="null" type="long"><generator class="date"/></id>
        <many-to-one name="lesson" />
        <set name="examStates" collection-type="set" cascade="all,delete-orphan">
            <key column="course_grade_state_id" />
            <one-to-many  class="org.openurp.eams.grade.ExamGradeState"/>
        </set>
        <set name="gaStates" collection-type="set" cascade="all,delete-orphan">
            <key column="course_grade_state_id" />
            <one-to-many  class="org.openurp.eams.grade.GaGradeState"/>
        </set>
        <many-to-one name="scoreMarkStyle"  not-null="true"/>
        <property name="status"/>
        <property name="operator"/>
        <property name="beginOn"/>
        <property name="endOn"/>
        <property name="updatedAt"/>
    </class>

</hibernate-mapping>